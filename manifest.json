{
  "manifest_version": 3,
  "name": "StartSmart",
  "version": "1.4",
  "permissions": [
    "storage",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "StartSmart Tabs"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}